{
    "name": "wc-tag-input",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "build:wc": "vue-cli-service build --target wc --mode production --name wc-tag-input ./src/components/Widget.vue",
        "build:wc:async": "vue-cli-service build --target wc-async --mode production --name wc-tag-input ./src/components/Widget.vue",
        "deploy": "npm run build && gh-pages",
        "gb-pages": "gh-pages -d site --dotfiles=true --cleanup"
    },
    "dependencies": {
        "buefy": "^0.8.9",
        "core-js": "^3.4.4",
        "gh-pages": "^3.1.0",
        "node-sass": "^4.13.0",
        "sass-loader": "^8.0.0",
        "vue": "^2.6.10",
        "vue-class-component": "^7.0.2",
        "vue-property-decorator": "^8.3.0"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^4.1.0",
        "@vue/cli-plugin-typescript": "^4.1.0",
        "@vue/cli-service": "^4.1.0",
        "typescript": "~3.5.3",
        "vue-template-compiler": "^2.6.10"
    }
}
